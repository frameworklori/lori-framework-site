{
"$schema": "https://json-schema.org/draft/2020-12/schema",
"title": "CASE-022 Juror Feedback Schema",
"type": "object",
"properties": {
"juror_id": {
"type": "string",
"description": "Anonymized or hashed juror ID"
},
"region": {
"type": "string",
"enum": ["Southern US", "Mongolia"],
"description": "Feedback region classification"
},
"feedback_category": {
"type": "string",
"enum": [
"water equity",
"cooling infrastructure",
"energy partnership",
"cultural preservation",
"infrastructure feasibility",
"climate volatility"
],
"description": "CASE-022-specific classification"
},
"comment": {
"type": "string",
"description": "Free-form juror feedback"
},
"ethical_weighting": {
"type": "number",
"minimum": 0,
"maximum": 1,
"description": "Optional ethical importance score"
},
"timestamp": {
"type": "string",
"format": "date-time",
"description": "Submission timestamp"
}
},
"required": ["juror_id", "region", "feedback_category", "comment", "timestamp"]
}
